{
  "name": "arnext-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "arweave": "npm run build:arweave && npx serve -s out",
    "deploy": "node node_modules/arnext-arkb deploy out",
    "deploy:turbo": "turbo upload-folder --folder-path out",
    "build:arweave": "cross-env NEXT_PUBLIC_DEPLOY_TARGET='arweave' next build && node arweave.mjs",
    "test": "mocha"
  },
  "dependencies": {
    "@ar.io/sdk": "^2.3.2",
    "arnext": "^0.1.2",
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "crypto-browserify": "^3.12.1",
    "https-browserify": "^1.0.0",
    "next": "14.2.13",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "react": "^18",
    "react-dom": "^18",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@ardrive/turbo-sdk": "^1.19.0",
    "arnext-arkb": "^0.0.1",
    "cheerio": "^1.0.0",
    "cross-env": "^7.0.3",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.13",
    "express": "^4.21.0",
    "http-proxy-middleware": "^3.0.2",
    "starknet": "^6.11.0"
  }
}
