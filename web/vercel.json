{
  "redirects": [
    {
      "source": "/:path*/",
      "destination": "/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/:profileName([a-zA-Z0-9-]+)",
      "destination": "/profile.html"
    },
    {
      "source": "/:profileName([a-zA-Z0-9-]+)/:gateway",
      "destination": "/profile.html"
    }
  ]
}